{
  "name": "event-ticket-booking-system-complete",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "demo": "DEMO_MODE=true node src/scripts/initDemo.js",
    "test": "jest --runInBand --detectOpenHandles --coverage",
  "docker:test": "docker-compose up --build -d db && docker-compose run --rm app sh -c \"npx sequelize-cli db:migrate && node src/scripts/initDemo.js && npm test\""

  },
  "dependencies": {
    "basic-auth": "^2.0.1",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "pino": "^8.14.0",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "sequelize-cli": "^6.6.0",
    "supertest": "^7.1.4",
    "sqlite3": "^5.1.6"
  }
}
